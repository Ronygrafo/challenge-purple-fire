{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/static-build",
        "config": { "zeroConfig": true }
      }
    ],
    "routes": [
      {
        "src": "/*",
        "headers": {
          "Access-Control-Allow-Origin": "*"
        }
      },
      {
        "src": "/[^.]+",
        "dest": "/",
        "status": 200
      }
    ]
  }